{
  "name": "shopify-express-boilerplate",
  "version": "0.0.6",
  "description": "Shopify Boilerplate Code written in React, Node, Express, MongoDB and GraphQL",
  "type": "module",
  "author": {
    "name": "Ravi Patel",
    "url": "https://webrexstudio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webrexRavi/SEA-MEAN.git"
  },
  "scripts": {
    "update": "ncu -u",
    "update:check": "ncu",
    "update:url": "node devUtils/updateDashboard.js",
    "dev": "concurrently \"npm run dev-run\" \"npm run ssl\"",
    "ssl": "local-ssl-proxy --source 4200 --target 8081",
    "dev-run": "NODE_ENV=dev nodemon server/index.js --ignore client/ --ignore dist/ --ignore server/index.js",
    "dev:server": "NODE_ENV=dev nodemon server/index.js --ignore client/ --ignore dist/ --ignore backup/",
    "dev:frontend": "vite --config=./client/vite.config.js",
    "preserve": "npm run build",
    "build": "vite build --config=./client/vite.config.js",
    "sentryrelease": "sentry-cli releases new -p seo-frontend 1.0.0",
    "sentrysourcemaps": "sentry-cli releases --org webrex-studio-llp --project seo-frontend files 1.0.0 upload-sourcemaps ./dist/client/",
    "start": "PORT=6002 NODE_ENV=prod node server/index.js",
    "start-prod": "PORT=6003 NODE_ENV=prod node server/index.js",
    "start:local": "concurrently \"NODE_ENV=prod node server/index.js\" \"npm run build-ssl\"",
    "build-ssl": "local-ssl-proxy --source 4200 --target 8081",
    "pretty": "prettier --write .",
    "ngrok:auth": "ngrok authtoken <your-auth-token>",
    "ngrok": "ngrok http 5173",
    "ngrok:prod": "ngrok http 8081",
    "shopify": "shopify",
    "s:e:create": "shopify app generate extension",
    "s:e:dev": "shopify app dev",
    "s:e:deploy": "shopify app deploy",
    "s:e:reset": "npm run s:e:dev -- --reset",
    "diff": "diff-package-lock 859b6e5638bb5944a3b9849cc7acf812aa8b94c6 8f5d78e684a1e6067687fafa494dff698208a3f7"
  },
