{
  "printWidth": 115,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "importOrder": ["^react(.*)", "antd/(.*)", "<THIRD_PARTY_MODULES>", "@/(.*)", "^[./]"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
